{
    "name": "ryanl/my-di",
    "description": "PSR 11",
    "type": "library",
    "require": {
        "psr/container": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ryanl\\MyDi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ryan Lima",
            "email": "me@ryanlima.dev"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2.23",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^5.15"
    },
    "scripts": {
        "test": "php ./vendor/bin/pest tests",
        "analyse" : "vendor/bin/phpstan analyse -c phpstan.neon"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
